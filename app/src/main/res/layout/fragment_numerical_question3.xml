<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    style="@style/FragmentStyle"
    tools:context="com.example.isabel.usertestapp.NumericalInputFragment">


    <TextView
        style="@style/TextFieldStyle"
        android:text="@string/numerical_question3"
        android:id="@+id/numericalQuestion3"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="77dp" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:id="@+id/inputNumerical3"
        android:layout_marginTop="59dp"
        android:layout_below="@+id/numericalQuestion3"
        android:layout_centerHorizontal="true"
        android:textAlignment="center"
        android:textSize="30sp" />

    <Button
        style="@style/ButtonNumberStyle"
        android:text="@string/one"
        android:id="@+id/btnOneNumerical3"
        android:layout_below="@+id/inputNumerical3"
        android:layout_marginLeft="180dp"
        android:layout_marginTop="64dp" />

    <Button
        style="@style/ButtonNumberStyle"
        android:text="@string/two"
        android:id="@+id/btnTwoNumerical3"
        android:layout_alignTop="@+id/btnOneNumerical3"
        android:layout_toEndOf="@+id/btnOneNumerical3" />

    <Button
        style="@style/ButtonNumberStyle"
        android:text="@string/three"
        android:id="@+id/btnThreeNumerical3"
        android:layout_alignTop="@+id/btnTwoNumerical3"
        android:layout_toEndOf="@+id/btnTwoNumerical3" />

    <Button
        style="@style/ButtonNumberStyle"
        android:text="@string/four"
        android:id="@+id/btnFourNumerical3"
        android:layout_alignTop="@+id/btnThreeNumerical3"
        android:layout_toEndOf="@+id/btnThreeNumerical3" />

    <Button
        style="@style/ButtonNumberStyle"
        android:text="@string/five"
        android:id="@+id/btnFiveNumerical3"
        android:layout_alignTop="@+id/btnFourNumerical3"
        android:layout_toEndOf="@+id/btnFourNumerical3" />

    <ImageButton
        style="@style/ButtonNumberStyle"
        android:id="@+id/backspaceNumerical3"
        android:src="@android:drawable/ic_input_delete"
        android:scaleType="centerCrop"
        android:layout_alignTop="@+id/btnFiveNumerical3"
        android:layout_toEndOf="@+id/btnFiveNumerical3" />

    <Button
        style="@style/ButtonNumberStyle"
        android:text="@string/six"
        android:id="@+id/btnSixNumerical3"
        android:layout_below="@+id/btnOneNumerical3"
        android:layout_alignStart="@+id/btnOneNumerical3"
        android:layout_marginTop="48dp" />

    <Button
        style="@style/ButtonNumberStyle"
        android:text="@string/seven"
        android:id="@+id/btnSevenNumerical3"
        android:layout_alignTop="@+id/btnSixNumerical3"
        android:layout_alignStart="@+id/btnTwoNumerical3" />

    <Button
        style="@style/ButtonNumberStyle"
        android:text="@string/eight"
        android:id="@+id/btnEightNumerical3"
        android:layout_alignTop="@+id/btnSevenNumerical3"
        android:layout_alignStart="@+id/btnThreeNumerical3" />

    <Button
        style="@style/ButtonNumberStyle"
        android:text="@string/nine"
        android:id="@+id/btnNineNumerical3"
        android:layout_alignTop="@+id/btnEightNumerical3"
        android:layout_toEndOf="@+id/btnEightNumerical3" />

    <Button
        style="@style/ButtonNumberStyle"
        android:text="@string/zero"
        android:id="@+id/btnZeroNumerical3"
        android:layout_alignTop="@+id/btnNineNumerical3"
        android:layout_alignStart="@+id/btnFiveNumerical3" />

    <Button
        style="@style/ButtonNumberStyle"
        android:text="@string/point"
        android:id="@+id/btnPointNumerical3"
        android:layout_alignTop="@+id/btnZeroNumerical3"
        android:layout_alignStart="@+id/backspaceNumerical3" />

    <Button
        style="@style/ButtonDontKnowStyle"
        android:text="@string/answer_dont_know"
        android:id="@+id/btnNumerical3DontKnow"
        android:layout_alignParentBottom="true"
        android:layout_alignParentEnd="true" />

    <TextView
        style="@style/TextFieldSwipeStyle"
        android:text="@string/swipe_to_continue"
        android:id="@+id/numerical3Continue"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:visibility="invisible"/>
